<mujoco model="linearparticle">

    <compiler inertiafromgeom="true" angle="radian" coordinate="local" />
    <option timestep="0.01" gravity="0 0 0" iterations="20" integrator="Euler" />
    <default>
        <joint limited="false" damping="1" />
        <geom contype="2" conaffinity="1" condim="1" friction=".5 .1 .1" density="1000" margin="0.002" />
    </default>

    <worldbody><body name="particle" pos="0.0 0.0 0.0"> 
<geom name="particle_geom" type="sphere" size="0.05" rgba="0.9 0.1 0.1 1"/>
<site name="particle_site" pos="0 0 0" size="0.01" />
<joint name="ball_x" type="slide" pos="0 0 0" axis="1 0 0" />
<joint name="ball_y" type="slide" pos="0 0 0" axis="0 1 0" />
</body>
<body name="x0" pos="-0.664937083073 0.75 0.0"> 
<geom name="x0_geom" type="sphere" size="0.05" rgba="0.9 0.1 0.1 1"/>
</body>
<body name="target" pos="0.439162205374 -0.75 0"> 
<geom name="target_geom" type="sphere" size="0.05" rgba="0 0.9 0.1 1"/>
</body>
<body name="obs0" pos="0.04 -0.24 0">
<geom name="obs0_geom" type="sphere" size="0.220532468706" contype="1"/>
</body>
<body name="obs1" pos="0.3 -0.3 0">
<geom name="obs1_geom" type="capsule" fromto="-0.3 0 0 0.3 0 0" size="0.1" contype="1"/>
</body>
<body name="obs2" pos="-0.2 0.0 0">
<geom name="obs2_geom" type="capsule" fromto="-0.3 0 0 0 0.3 0" size="0.1" contype="1"/>
</body>


    </worldbody>

    <actuator>
        <motor joint="ball_x" ctrlrange="-1.0 1.0" ctrllimited="true"/>
        <motor joint="ball_y" ctrlrange="-1.0 1.0" ctrllimited="true"/>
    </actuator>
</mujoco>
